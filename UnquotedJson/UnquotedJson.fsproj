  <ItemGroup />

  <ItemGroup>
    <PackageReference Include="FSharp.Literals" Version="2.2.5" />
    <PackageReference Include="FslexFsyacc.Runtime" Version="1.0.4" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="6.0.1" />
  </ItemGroup>

</Project>
